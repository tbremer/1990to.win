{
  "name": "1990towin",
  "scripts": {
    "start": "node ./server/index.js",
    "build:client": "webpack -p",
    "dev:client": "webpack --watch --mode=development",
    "dev:server": "NODE_ENV=dev nodemon -w ./ -e css,js,json,html ./server/index.js",
    "predev": "cmp -s package.json ._package.json || (npm install && cp package.json ._package.json)",
    "dev": "npm-run-all -p dev:*",
    "lint:client": "eslint --ext .ts client"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "panel": "^2.6.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {},
  "lint-staged": {
    "*.{js,ts,css,json}": [
      "prettier --write"
    ],
    "client/**/*.ts": [
      "eslint --ext .ts --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
